URxvt*loginShell: true
URxvt*font: xft:monospace:size=10
URxvt*saveLines: 0
! provided by rxvt-unicode-fontspacing-noinc-vteclear-secondarywheel
URxvt*secondaryWheel: true
! disable this mode as it has a ctrl-shift keybinding
URxvt*iso14755: false

! alacritty default colours: Tomorrow Night Bright
URxvt*background: #000000
URxvt*foreground: #eaeaea
! black dark/light
URxvt*color0:             #000000
URxvt*color8:             #666666
! red dark/light
URxvt*color1:             #d54e53
URxvt*color9:             #ff3334
! green dark/light
URxvt*color2:             #b9ca4a
URxvt*color10:            #9ec400
! yellow dark/light
URxvt*color3:             #e6c547
URxvt*color11:            #e7c547
! blue dark/light
URxvt*color4:             #7aa6da
URxvt*color12:            #7aa6da
! magenta dark/light
URxvt*color5:             #c397d8
URxvt*color13:            #b77ee0
! cyan dark/light
URxvt*color6:             #70c0ba
URxvt*color14:            #54ced6
! white dark/light
URxvt*color7:             #ffffff
URxvt*color15:            #ffffff
